{
    "expo": {
        "name": "payXwise",
        "slug": "payXwise",
        "scheme": "payXwise",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/transfer.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/transfer.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },

        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/transfer.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.creati1ve123212.payXwise",
            "permissions": ["android.permission.CAMERA"]
        },
        "web": {
            "favicon": "./assets/transfer.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-camera",
                {
                    "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
                    "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
                    "recordAudioAndroid": true
                }
            ]
        ],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "a241e32c-b297-47ff-b59b-437fbd3e05e2"
            }
        }
    }
}
